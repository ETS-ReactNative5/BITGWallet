{
  "name": "react-native-v8",
  "version": "v0.62.2-patch.1",
  "description": "Opt-in V8 runtime for React Native Android",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kudo/react-native-v8.git"
  },
